{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["[", "]"],
    ["{", "}"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "\"", "close": "\"", "notIn": ["string", "comment"] },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "(", "close": ")" }
  ],
  "wordPattern": "\\S+",
  "autoCloseBefore": "\";:.,=}])> \n\t",
  "surroundingPairs": [],
  "colorizedBracketPairs": [],
  "folding": {
    "offSide": true,
    "markers": {
      "start": "^\\s*#\\s*#?region\\b",
      "end": "^\\s*#\\s*#?endregion\\b"
    }
  },
  "onEnterRules": [
    {
      "beforeText": "^\\s*\\S+:\\s*$",
      "action": {
        "indent": "indent"
      }
    }
  ]
}
